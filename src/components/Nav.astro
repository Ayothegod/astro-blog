---
import navData from "../data/navData"
// console.log(navData);

---

<nav>
    <div class="shadow-globalshadow h-12 flex items-center justify-between px-2"> 
        <a href="/" class="text-2xl font-mono font-extrabold">
            Logo
        </a>

        <ul class="">
            {
                navData.map(item => (
                    <a href={`${item.path}`}>{item.name}</a>
                ))
            }
        </ul>
    </div>
</nav>




